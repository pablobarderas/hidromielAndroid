<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">

    <!-- Base button  -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/btnBack" />
            <size android:width="@dimen/btn_width" android:height="@dimen/btn_height"/>
            <corners android:radius="20dp" />
            <stroke android:width="@dimen/btn_stroke_width" android:color="@color/btnStroke" />
        </shape>
    </item>

</layer-list>

